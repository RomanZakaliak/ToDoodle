@model IEnumerable<Todo.Models.ApplicationUser>

<h2>User List</h2>
<div class="container">
    <table class="table">
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access rights</a>
                </td>
            </tr>
        }
    </table>
</div>
